#Testing File
TFILE = morseTest


morseTest: morse.o morseTest.o
	gcc morse.o morseTest.o -lm -o $(TFILE) 

morse.o: 

depend: 
	$(CC) -MM $(CPPFLAGS) *.c

clean:
	rm -f *.o $(TFILE)



#Dependencies generated by make depend
file-utils.o: file-utils.c file-utils.h
main.o: main.c file-utils.h morse.h
morse.o: morse.c morse.h
morseTest.o: morseTest.c morse.h  
